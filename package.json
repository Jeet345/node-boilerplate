{
  "name": "server-boilerplate",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "db:migrate": "./node_modules/.bin/sequelize --options-path ./src/schema/main-server/.sequelizerc db:migrate",
    "dev": "nodemon src/",
    "dev-docker:up": "docker-compose -f docker-compose.dev.yml --env-file ./docker/.env.docker up -d --build",
    "dev-docker:down": "docker-compose -f docker-compose.dev.yml --env-file ./docker/.env.docker down",
    "dev-docker:logs": "docker logs -f server-boilerplate-dev",
    "dev-docker:install": "docker exec -it server-boilerplate-dev npm i",
    "lint": "esw --ext .json,.js,.gql,.graphql src --color",
    "lint:watch": "npm run lint -- --watch",
    "lint:fix": "eslint src --ext .json,.js,.gql,.graphql --fix",
    "pre-commit": "npm run lint",
    "pre-push": "lint-staged && npm run lint",
    "predev-docker:up": "sh ./docker/startup.sh",
    "prestart": "npm run db:migrate",
    "start": "node src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:logicwind/general/backend/server-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "4.7.1",
    "@aws-sdk/client-cloudfront": "3.342.0",
    "@aws-sdk/client-s3": "3.342.0",
    "@aws-sdk/cloudfront-signer": "3.342.0",
    "@aws-sdk/s3-request-presigner": "3.342.0",
    "@graphql-tools/load-files": "6.6.1",
    "@graphql-tools/merge": "8.4.2",
    "@graphql-tools/schema": "9.0.19",
    "@graphql-tools/utils": "10.0.0",
    "@mux/mux-node": "7.3.0",
    "@sentry/node": "7.53.1",
    "axios": "1.4.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "crypto": "1.0.1",
    "dotenv": "16.1.1",
    "express": "4.18.2",
    "express-rate-limit": "6.7.0",
    "express-useragent": "1.0.15",
    "graphql": "16.6.0",
    "graphql-query-complexity": "0.12.0",
    "graphql-rate-limit": "3.3.0",
    "graphql-redis-subscriptions": "2.6.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-ws": "5.13.1",
    "http-status": "1.6.2",
    "i18n": "0.15.1",
    "ioredis": "5.3.2",
    "jsonwebtoken": "9.0.0",
    "lodash": "4.17.21",
    "mime": "3.0.0",
    "moment": "2.29.4",
    "multer": "1.4.5-lts.1",
    "nanoid": "4.0.2",
    "pg": "8.11.0",
    "pg-hstore": "2.3.4",
    "pusher": "5.1.3",
    "qs": "6.11.2",
    "randomstring": "1.2.3",
    "rate-limit-redis": "3.0.2",
    "sequelize": "6.31.1",
    "sequelize-cli": "6.6.0",
    "uuid": "9.0.0",
    "winston": "3.9.0",
    "ws": "8.13.0"
  },
  "devDependencies": {
    "@graphql-eslint/eslint-plugin": "3.15.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-eslint-plugin": "5.0.8",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-security": "1.7.1",
    "eslint-watch": "7.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.1",
    "nodemon": "2.0.20"
  },
  "engines": {
    "node": "20.2.0",
    "npm": "9.6.6"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  }
}
